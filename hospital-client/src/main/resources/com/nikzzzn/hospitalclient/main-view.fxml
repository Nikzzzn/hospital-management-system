<VBox alignment="CENTER" prefHeight="410.0" prefWidth="640.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.nikzzzn.hospitalclient.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="List of appointments">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TableView id="appointmentsTable" prefHeight="313.0" prefWidth="600.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Button id="btnAddAppointment" mnemonicParsing="false" text="Add new appointment" />
   </children>
</VBox>
